Ccs {idlist   = Var ["x", "y", "z"], 
     funlist  = Sig [("add", 2, 1)],
     rulelist = 
       Rules [(Arrow (ID "add{}{0}" [VarID "y" []]) 
                     (ID "<>" [ID "0" [], VarID "y" []]) []), 
              (Arrow (ID "add{}{0}" [ID "s" [VarID "z" []]]) 
                     (ID "<>" [ID "s" [VarID "x" []], VarID "y" []]) 
                     [CArrow (ID "add" [VarID "x" [], VarID "y" []]) 
                             (ID "<>" [VarID "z" []])]),
              (Arrow (ID "add{0,1}{}" [ID "0" [],VarID "y" []]) 
                     (ID "<>" [VarID "y" []]) []),
              (Arrow (ID "add{0,1}{}" [ID "s" [VarID "x" []],VarID "y" []]) 
                     (ID "<>" [ID "s" [VarID "z" []]]) 
                     [CArrow (ID "add" [VarID "x" [],VarID "y" []]) 
                             (ID "<>" [VarID "z" []])])
]}
