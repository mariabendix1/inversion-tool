Ccs {idlist = Var ["x","y","z","u"], 
     funlist = Sig [("perm",1,1), ("del",1,2)], 
     rulelist = Rules 
       [Arrow (ID "perm{}{0}" [ID "[]" []]) 
              (ID "<>" [ID "[]" []]) [],
        Arrow (ID "perm{}{0}" [ID ":" [VarID "y" [],VarID "z" []]]) 
              (ID "<>" [VarID "x" []]) 
              [CArrow (ID "del" [VarID "x" []]) 
                      (ID "<>" [VarID "y" [],VarID "u" []]),
               CArrow (ID "perm" [VarID "u" []]) 
                      (ID "<>" [VarID "z" []])],

        Arrow (ID "del{0}{}" [ID ":" [VarID "x" [],VarID "y" []]]) 
              (ID "<>" [VarID "x" [],VarID "y" []]) [],
        Arrow (ID "del{0}{}" [ID ":" [VarID "x" [],VarID "y" []]]) 
              (ID "<>" [VarID "z" [],ID ":" [VarID "x" [],VarID "u" []]]) 
              [CArrow (ID "del" [VarID "y" []]) 
                      (ID "<>" [VarID "z" [],VarID "u" []])],

        Arrow (ID "perm{0}{}" [ID "[]" []]) 
              (ID "<>" [ID "[]" []]) [],
        Arrow (ID "perm{0}{}" [VarID "x" []]) 
              (ID "<>" [ID ":" [VarID "y" [],VarID "z" []]]) 
              [CArrow (ID "del" [VarID "x" []]) 
                      (ID "<>" [VarID "y" [],VarID "u" []]),
               CArrow (ID "perm" [VarID "u" []]) 
                      (ID "<>" [VarID "z" []])]]}
