Ccs {idlist   = Var ["x", "y", "z", "w"], 
     funlist  = Sig [("mul", 2, 1), ("add", 2, 1)],
     rulelist = 
       Rules [(Arrow (ID "mul{}{0}" [ID "0" []]) 
                     (ID "<>" [ID "0" [], VarID "y" []]) []), 
              (Arrow (ID "mul{}{0}" [VarID "z" []]) 
                     (ID "<>" [ID "s" [VarID "x" []], VarID "y" []]) 
                     [CArrow (ID "add{}{0}" [VarID "z" []]) 
                             (ID "<>" [VarID "y" [], VarID "w" []]), 
                      CArrow (ID "mul{}{0}" [VarID "w" []]) 
                             (ID "<>" [VarID "x" [], VarID "y" []])]), 
              (Arrow (ID "add{}{0}" [VarID "y" []]) 
                     (ID "<>" [ID "0" [], VarID "y" []]) []), 
              (Arrow (ID "add{}{0}" [ID "s" [VarID "z" []]]) 
                     (ID "<>" [ID "s" [VarID "x" []], VarID "y" []]) 
                     [CArrow (ID "add{}{0}" [VarID "z" []]) 
                             (ID "<>" [VarID "x" [], VarID "y" []])])]}
