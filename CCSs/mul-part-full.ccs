(VAR x y z w)
(SIG (mul 2 1) (add 2 1))
(RULES
  mul{}{0}(0) -> <0,y>
  mul{}{0}(z) -> <s(x), y> <= add{}{0}(z) -> <y, w> ^
                              mul{1}{0}(y,w) -> <x>
  add{}{0}(y) -> <0,y>
  add{}{0}(s(z)) -> <s(x), y> <= add{}{0}(z) -> <x, y>

  mul{1}{0}(y,0) -> <0>
  mul{1}{0}(y,z) -> <s(x)> <= add{0}{0}(y,z) -> <w> ^
                              mul{1}{0}(y,w) -> <x>
  add{0}{0}(0,y) -> <y>
  add{0}{0}(s(x),s(z)) -> <y> <= add{0}{0}(x,z) -> <y>
) 
