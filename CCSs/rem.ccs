(VAR x xs i y zs)
(SIG (rem 2 2))
(RULES
  rem(:(x,xs),0) -> <x,xs>
  rem(:(x,xs),s(i)) -> <y,:(x,zs)> <= rem(xs, i) -> <y,zs>
)
