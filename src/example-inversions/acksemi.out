(VAR y1 x x1 y yw z)
(SIG 
  (ack 2 1))
(RULES 
  ack{1}{1}(0, s(y)) -> <y>
  ack{1}{1}(s(x1), z) -> <0> <= ack{1,2}{1}(x1, s(0), z) -> < >
  ack{1}{1}(s(x1), z) -> <s(yw)> <= ack{1}{1}(x1, z) -> <y1> ^ ack{1}{1}(s(x1), y1) -> <yw>
  ack{1,2}{1}(0, y, s(y)) -> < >
  ack{1,2}{1}(s(x1), 0, z) -> < > <= ack{1,2}{1}(x1, s(0), z) -> < >
  ack{1,2}{1}(s(x1), s(yw), z) -> < > <= ack{1}{1}(x1, z) -> <y1> ^ ack{1,2}{1}(s(x1), yw, y1) -> < >
)
